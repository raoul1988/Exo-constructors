package com.efunzo.be.training;

public class Car extends Vehicule {
	
	private String mark;
	private String typevitesse;
	private String color;
	private String directionmeans;
	
	public Car() {
		
	}

	
	public Car(String mark, String typevitesse, String color, String directionmeans) {
		super();
		this.mark = mark;
		this.typevitesse = typevitesse;
		this.color = color;
		this.directionmeans = directionmeans;
	}
	
	public void mark() {
		
		System.out.println("Car.mark() called");
	}
	 
	public void typevitesse() {
		
		System.out.println("Car. typevitesse() called");
	}
	
	public void color() {
		
		System.out.println("Car.color() called");
		
	}
	
	public void directionmeans() {
		
		System.out.println("Car.directionmeans() called");
	}
	

	public String getMark() {
		return mark;
	}

	public void setMark(String mark) {
		this.mark = mark;
	}

	public String gettypeVitesse() {
		return typevitesse;
	}

	public void settypeVitesse(String typevitesse) {
		this.typevitesse = typevitesse;
	}

	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}

	public String getDirectionmeans() {
		return directionmeans;
	}

	public void setDirectionmeans(String directionmeans) {
		this.directionmeans = directionmeans;
	}


	@Override
	public String toString() {
		return "Car [mark=" + mark + ", typevitesse=" + typevitesse + ", color=" + color + ", directionmeans="
				+ directionmeans + "]";
	}


	}
	
